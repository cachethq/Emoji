{
    "name": "steffjenl/emoji",
    "description": "An Emoji Parser For Laravel 7",
    "keywords": ["emoji", "parser", "Emoji", "Cachet", "CachetHQ"],
    "license": "MIT",
    "authors": [
        {
            "name": "Cachet",
            "email": "support@cachethq.io"
        }
    ],
    "require": {
        "php": "^8.0",
        "graham-campbell/guzzle-factory": "^5.1",
        "illuminate/contracts": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "league/commonmark": "^1.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^3.1",
        "graham-campbell/markdown": "^13.1",
        "graham-campbell/testbench": "^5.1",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0|^8.0|9.0"
    },
    "autoload": {
        "psr-4": {
            "CachetHQ\\Emoji\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CachetHQ\\Tests\\Emoji\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "CachetHQ\\Emoji\\EmojiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
